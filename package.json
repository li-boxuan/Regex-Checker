{
  "name": "regex-checker",
  "version": "1.0.0",
  "description": "A PR regex checker GitHub App",
  "author": "Boxuan Li <liboxuan@connect.hku.hk>",
  "license": "ISC",
  "repository": "https://github.com//Regex-Check.git",
  "homepage": "https://github.com//Regex-Check",
  "bugs": "https://github.com//Regex-Check/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "babel -w lib/ -d dist/ | nodemon --watch dist",
    "build": "babel lib/ -d dist/",
    "start": "npm run build && probot run ./dist/index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^22.4.3",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
